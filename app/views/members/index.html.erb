<div class="columns">
  <%= render "member", member: current_member %>
  <% columns = 1 %>
  <% @members.each do |member| %>
    <% if (columns % 3).zero? %>
      <div class="columns">
    <% end %>

    <%= render "member", member: member unless member == current_member %>

    <% columns += 1 %>
    <% if (columns % 3).zero? %>
      </div>
    <% end %>
  <% end %>